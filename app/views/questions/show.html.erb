<div class="q-a">
  <h3>Question ID<%= @question.id %></h3>
  <p><%= @question.content %></p>

    <h3>Email:</h3>
    <p><%= @question.email %></p>

  <h3>Answers:</h3>
  <% if @question.answers.any? %>
    <% @question.answers.each do |q| %>
      <p>q</p>
    <% end %>
  <% else %>
    <p>No Answers yet.</p>
  <% end %>

  <%= link_to 'Back', admin_path %>
</div>
