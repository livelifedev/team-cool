<% @documents.each do |document| %>
  <p><%= document.title %></p>
  <p><%= document.description %></p>
  <p><% document.subjects.each do |sub| %>
    <li><%= sub.name %></li>
  <% end %></p>
  <p><%= document.doc_type %></p>
  <%= link_to "edit", edit_document_path(document.id) %>
  <%= link_to "show", document_path(document.id) %>
  <p>---------------------------</p>  
<% end %>